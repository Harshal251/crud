<h1>EDIT</h1>

 
<h1><%= @l.name %></h1>

 
<%= form_with model: @l , method: :post,url:update_path(@l)  do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email%>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>